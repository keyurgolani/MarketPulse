{
  "name": "marketpulse-server",
  "version": "1.0.0",
  "description": "MarketPulse backend server with Express.js, TypeScript, and SQLite",
  "main": "dist/index.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "./node_modules/.bin/nodemon -r tsconfig-paths/register src/index.ts",
    "build": "./node_modules/.bin/tsc && ./node_modules/.bin/tsc-alias",
    "start": "node dist/index.js",
    "test": "./node_modules/.bin/jest",
    "test:watch": "./node_modules/.bin/jest --watch",
    "test:coverage": "./node_modules/.bin/jest --coverage",
    "test:integration": "./node_modules/.bin/jest --testPathPatterns=integration",
    "lint": "./node_modules/.bin/eslint src/**/*.ts",
    "lint:fix": "./node_modules/.bin/eslint src/**/*.ts --fix",
    "type-check": "./node_modules/.bin/tsc --noEmit",
    "migrate": "./node_modules/.bin/ts-node --project tsconfig.scripts.json scripts/migrate.ts",
    "migrate:status": "./node_modules/.bin/ts-node --project tsconfig.scripts.json scripts/migrate.ts status",
    "migrate:validate": "./node_modules/.bin/ts-node --project tsconfig.scripts.json scripts/migrate.ts validate"
  },
  "keywords": [
    "financial",
    "dashboard",
    "api",
    "typescript",
    "express"
  ],
  "author": "MarketPulse Team",
  "license": "MIT",
  "dependencies": {
    "@types/socket.io": "^3.0.2",
    "@types/sqlite3": "^5.1.0",
    "@types/uuid": "^10.0.0",
    "axios": "^1.11.0",
    "cheerio": "^1.1.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.7.0",
    "joi": "^18.0.0",
    "mongodb": "^6.18.0",
    "morgan": "^1.10.1",
    "natural": "^8.1.0",
    "node-html-parser": "^7.0.1",
    "rate-limiter-flexible": "^7.2.0",
    "redis": "^5.8.1",
    "sentiment": "^5.0.2",
    "socket.io": "^4.8.1",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/mongodb": "^4.0.7",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.3.0",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "eslint": "^9.33.0",
    "jest": "^30.0.5",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  }
}
