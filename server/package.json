{
  "name": "marketpulse-server",
  "version": "1.0.0",
  "description": "MarketPulse backend server with Express.js, TypeScript, and SQLite",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon -r tsconfig-paths/register src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "migrate": "ts-node --project tsconfig.scripts.json scripts/migrate.ts",
    "migrate:status": "ts-node --project tsconfig.scripts.json scripts/migrate.ts status",
    "migrate:validate": "ts-node --project tsconfig.scripts.json scripts/migrate.ts validate"
  },
  "keywords": [
    "financial",
    "dashboard",
    "api",
    "typescript",
    "express"
  ],
  "author": "MarketPulse Team",
  "license": "MIT",
  "dependencies": {
    "@types/sqlite3": "^5.1.0",
    "@types/uuid": "^10.0.0",
    "axios": "^1.11.0",
    "cheerio": "^1.1.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.7.0",
    "joi": "^18.0.0",
    "mongodb": "^6.18.0",
    "morgan": "^1.10.1",
    "natural": "^8.1.0",
    "node-html-parser": "^7.0.1",
    "rate-limiter-flexible": "^7.2.0",
    "redis": "^5.8.1",
    "sentiment": "^5.0.2",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/mongodb": "^4.0.7",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.3.0",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "jest": "^30.0.5",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  }
}
